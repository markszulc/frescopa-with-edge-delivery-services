                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="core/franklin/components/text/v1/text"
                                text="&lt;p>“We don’t roast beans. We awaken them.” – Carlos Vargas, Third-Generation Farmer&lt;/p>"/>
                        </col2>
                    </row1>
